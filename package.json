{
  "name": "docwalk",
  "version": "0.1.0",
  "description": "Your codebase, documented. Automatically. Analyze any repo, generate MkDocs Material sites, deploy anywhere, and keep docs in sync with git-diff intelligence.",
  "author": "DocWalk",
  "license": "MIT",
  "type": "module",
  "bin": {
    "docwalk": "./bin/docwalk.js"
  },
  "main": "src/index.ts",
  "files": [
    "dist",
    "bin",
    "scripts",
    "src/generators/templates"
  ],
  "scripts": {
    "dev": "tsx src/cli/index.ts",
    "build": "tsup src/index.ts src/cli/index.ts --format esm,cjs --dts --clean",
    "lint": "eslint src/ --ext .ts",
    "lint:fix": "eslint src/ --ext .ts --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc --noEmit",
    "postinstall": "node scripts/postinstall.cjs",
    "prepublishOnly": "npm run build",
    "docwalk": "tsx src/cli/index.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "0.78.0",
    "@google/generative-ai": "0.21.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "conf": "^13.0.1",
    "cosmiconfig": "^9.0.0",
    "execa": "^9.3.0",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "inquirer": "^9.3.0",
    "js-yaml": "^4.1.0",
    "listr2": "^8.2.0",
    "lodash-es": "^4.17.21",
    "log-symbols": "^6.0.0",
    "nanoid": "^5.0.7",
    "node-fetch": "^3.3.2",
    "openai": "6.25.0",
    "ora": "^8.0.1",
    "simple-git": "^3.25.0",
    "tree-sitter-wasms": "0.1.13",
    "web-tree-sitter": "^0.22.6",
    "zod": "^3.23.0"
  },
  "optionalDependencies": {
    "tree-sitter": "^0.21.1",
    "tree-sitter-c-sharp": "^0.21.1",
    "tree-sitter-go": "^0.21.0",
    "tree-sitter-java": "^0.21.0",
    "tree-sitter-javascript": "^0.21.3",
    "tree-sitter-php": "^0.22.0",
    "tree-sitter-python": "^0.21.0",
    "tree-sitter-ruby": "^0.21.0",
    "tree-sitter-rust": "^0.21.0",
    "tree-sitter-typescript": "^0.21.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "9.0.9",
    "@types/js-yaml": "^4.0.9",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.14.0",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.57.0",
    "prettier": "^3.3.0",
    "tsup": "^8.1.0",
    "tsx": "^4.15.0",
    "typescript": "^5.5.0",
    "vitest": "^1.6.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "documentation",
    "docs",
    "zensical",
    "codegen",
    "static-site",
    "api-docs",
    "git-diff",
    "auto-docs",
    "developer-tools"
  ]
}
